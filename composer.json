{
  "name": "og/core-domain",
  "description": "Core domain library for all APIs and implementations that are common between all the packages",
  "keywords": [
    "og",
    "olsthoorn-group",
    "core-domain"
  ],
  "homepage": "https://github.com/olsthoorn-group/core-domain",
  "license": "MIT",
  "authors": [
    {
      "name": "Mitchell Olsthoorn",
      "homepage": "https://github.com/mitchellolsthoorn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ramsey/uuid": "^2.8",
    "beberlei/assert": "^2.4"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0",
    "fabpot/php-cs-fixer": "~1.10",
    "satooshi/php-coveralls": "~0.6",
    "scrutinizer/ocular": "~1.0"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "OG\\Core\\Domain\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OG\\Core\\Test\\Domain\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.2-dev"
    }
  }
}
